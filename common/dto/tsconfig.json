{
  "extends": "../../tsconfig.json",
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "dist"],
  "compileOnSave": true,
  "compilerOptions": {
	  "rootDir": "src",
	  "target": "es2017",
	  "module": "commonjs",
	  "sourceMap": true,
	  "declaration": true,
	  "removeComments": true,
	  "emitDecoratorMetadata": true,
	  "experimentalDecorators": true,
	  "incremental": true,
		"skipLibCheck": true,
		"outDir": "../../dist/common/dto",
		"baseUrl": "./"
	}
}
